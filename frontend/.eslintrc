{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
  ],
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "mocha": true,
  },
  "rules": {
      "no-console": ["warn"],
      "linebreak-style": ["error", "windows"],
      "comma-dangle": ["warn", "always-multiline"],
      "react/jsx-filename-extension": ["error", { "extensions": [".js", ".jsx"] }],
      "react/no-unused-state": ["warn"],
    },
}
